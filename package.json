{
  "dependencies": {
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "ecstatic": "^2.1.0",
    "sheetify": "6.0.1",
    "tachyons": "^4.6.2",
    "watchify": "^3.9.0",
    "yo-yo": "1.x",
    "yo-yoify": "^3.5.0"
  },
  "main": "main.js",
  "scripts": {
    "watch": "budo main.js:bundle.js --dir ./public --live -- -t sheetify/transform",
    "build": "browserify main.js -t yo-yoify -t sheetify/transform -g uglifyify -o public/bundle.js",
    "start": "ecstatic ./public --port 8080"
  }
}
