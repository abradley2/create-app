{
  "dependencies": {
    "classnames": "^2.2.5",
    "ecstatic": "^2.1.0",
    "mithril": "^1.1.3",
    "sheetify": "^6.1.1",
    "tachyons": "^4.8.0"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "budo": "^10.0.4",
    "envify": "^4.1.0",
    "es2040": "^1.2.6",
    "standard": "^10.0.3",
    "uglifyify": "^4.0.3"
  },
  "author": "Tony Bradley",
  "name": "create-yo-yo-app",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development budo ./src/main.js:bundle.js --dir ./public --live",
    "build": "NODE_ENV=production browserify ./src/main.js -g uglifyify -o ./public/bundle.js && ecstatic ./public --port 8080",
    "test": "standard",
    "lint": "standard --fix"
  },
  "browserify": {
    "transform": [
      "es2040",
      "envify",
      "sheetify/transform"
    ]
  }
}
